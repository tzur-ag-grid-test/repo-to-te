{
  "name": "gx-table",
  "version": "7.2.0",
  "repository": {
    "type": "git",
    "url": "https://github.build.ge.com/grid-foundation/gx-table.git"
  },
  "scripts": {
    "start:local-proxy": "ng serve --proxy-config proxy.conf.js --ssl",
    "start:remote-proxy": "ng serve --proxy-config node_modules/foundation-common/proxy.conf.js --ssl --port 443",
    "validate": "node node_modules/foundation-common/build-validation.js",
    "dev": "ng serve",
    "ng-high-memory": "node --max_old_space_size=12000 ./node_modules/@angular/cli/bin/ng",
    "build:demo-page": "ng build --base-href=https://pages.github.build.ge.com/grid-foundation/gx-table/ --configuration=pkg",
    "build": "npm run validate && ng build",
    "deploy:demo-page": "gh-pages -d dist/demo",
    "package": "npm run validate && ng-packagr -c tsconfig.lib.json -p ng-package.json",
    "postpackage": "cp -a src/locale/. dist/pkg/locale/ && cp -a src/styles/. dist/pkg/styles/",
    "test": "npm run lint && ng test --watch true --code-coverage",
    "test:ci": "ng test --code-coverage --watch=false --browsers ChromiumHeadlessCI --progress=false",
    "test:watch": "ng test",
    "e2e": "ng e2e",
    "lint": "eslint -c .eslintrc.js --ext .ts ./src",
    "i18n": "ng run demo-data-table:i18n"
  },
  "private": false,
  "dependencies": {
    "@angular/core": "^18.0.5",
    "@angular/material-moment-adapter": "^18.0.5",
    "ag-grid-angular": "~31.3.2",
    "ag-grid-community": "31.3.4",
    "ag-grid-enterprise": "31.3.4",
    "ag-charts-community": "~9.3.1",
    "angular2-uuid": "^1.1.1",
    "gx-services": "edge",
    "gx-ui": "edge",
    "lodash": "^4.17.11",
    "lodash.isequal": "^4.5.0",
    "moment": "~2.29.3",
    "moment-timezone": "^0.5.28",
    "ng2-nouislider": "^1.8.2",
    "nouislider": "^13.1.5",
    "rxjs": "7.8.1",
    "tslib": "^2.0.3",
    "xliff": "^5.5.3",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.0.6",
    "@angular-eslint/builder": "^12.3.1",
    "@angular-eslint/eslint-plugin": "^12.3.1",
    "@angular/animations": "^18.0.5",
    "@angular/cli": "^18.0.6",
    "@angular/common": "^18.0.5",
    "@angular/compiler": "^18.0.5",
    "@angular/compiler-cli": "^18.0.5",
    "@angular/forms": "^18.0.5",
    "@angular/language-service": "^18.0.5",
    "@angular/localize": "^18.0.5",
    "@angular/material": "^18.0.5",
    "@angular/platform-browser": "^18.0.5",
    "@angular/platform-browser-dynamic": "^18.0.5",
    "@angular/router": "^18.0.5",
    "@types/elasticsearch": "^5.0.18",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^12.20.7",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "alasql": "^1.7.2",
    "codelyzer": "^6.0.0",
    "core-js": "^2.5.4",
    "eslint": "^7.31.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.23.4",
    "foundation-common": "edge",
    "gh-pages": "^2.1.1",
    "jasmine-core": "~5.1.1",
    "jasmine-spec-reporter": "^6.0.0",
    "karma": "^6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-cli": "^1.0.1",
    "karma-coverage": "^2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-junit-reporter": "^1.2.0",
    "lodash": "^4.17.11",
    "ng-packagr": "^18.1.0",
    "protractor": "~7.0.0",
    "ts-node": "^9.1.1",
    "typescript": "~5.5.3"
  }
}
